
@{
    ViewBag.Title = "Meni";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<body>

	<div class="jumbotron">
		<h2>Baza Profila</h2>
		<p class="lead">@ViewBag.Status : @ViewBag.User</p>
	</div>

	@if (@ViewBag.poruka != null) {

	<div style="border: 1px solid red">
		@ViewBag.poruka
	</div>
	}

	@Html.ActionLink("Lista Monoblokova", "MonoblockList", "ProfiliMeni", new { area = "" }, new { @class = "btn btn-info btn-sm" })

	@Html.ActionLink("Back", "UserPanel", "Users", new { area =""}, new { @class = "btn btn-info btn-sm" })


</body>

